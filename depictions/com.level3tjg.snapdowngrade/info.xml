<package>
	<id>com.level3tjg.snapdowngrade</id>
	<name>snapdowngrade</name>
	<version>1.0.2</version>
	<compatibility>
		<firmware>
			<miniOS>7.0</miniOS>
			<otherVersions>unsupported</otherVersions>
			<!--
			for otherVersions, you can put either unsupported or unconfirmed
			-->
		</firmware>
	</compatibility>
	<dependencies></dependencies>
	<descriptionlist>
		<description>Allows you to downgrade to any snapchat version under 10.30.0 without crashes (hopefully)</description>
	</descriptionlist>
	<screenshots></screenshots>
	<changelog>
		<change>Fixed startup crash and added version check</change>
	</changelog>
	<links></links>
</package>
